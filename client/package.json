{
  "name": "dashboard",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "dist": "ng build --prod --aot",
    "hmr": "ng serve --hmr -e=hmr",
    "compodoc": "compodoc -p src/tsconfig.json",
    "showdoc": "compodoc -s -d ./documentation",
    "lint": "tslint \"src/**/*.ts\"; stylelint \"src/styles.css\"; stylelint \"src/app/**/*.css\"",
    "lint-staged": "lint-staged",
    "stylelint-staged": "stylelint",
    "test": "ng test",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "2.4.5",
    "@angular/compiler": "2.4.5",
    "@angular/core": "2.4.5",
    "@angular/forms": "2.4.5",
    "@angular/http": "2.4.5",
    "@angular/platform-browser": "2.4.5",
    "@angular/platform-browser-dynamic": "2.4.5",
    "@angular/router": "3.4.5",
    "angular2-google-chart": "^1.1.2",
    "angular2-material-datepicker": "0.5.0",
    "bootstrap": "3.4.1",
    "chart.js": "2.4.0",
    "core-js": "2.4.1",
    "font-awesome": "4.7.0",
    "ng2-charts": "1.5.0",
    "rxjs": "5.0.3",
    "ts-helpers": "1.1.2",
    "web-animations-js": "2.2.2",
    "zone.js": "0.7.6"
  },
  "devDependencies": {
    "@angular/compiler-cli": "2.4.5",
    "@angularclass/hmr": "1.2.2",
    "@types/jasmine": "2.5.41",
    "@types/node": "7.0.4",
    "angular-cli": "1.0.0-beta.26",
    "codelyzer": "2.0.0-beta.4",
    "compodoc": "0.0.35",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "3.2.0",
    "karma": "1.4.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-cli": "1.0.1",
    "karma-jasmine": "1.1.0",
    "karma-remap-istanbul": "0.4.0",
    "lint-staged": "3.2.8",
    "pre-commit": "1.2.2",
    "protractor": "5.0.0",
    "stylelint": "7.7.1",
    "stylelint-config-standard": "^15.0.1",
    "ts-node": "2.0.0",
    "tslint": "4.4.0",
    "tslint-eslint-rules": "3.2.3",
    "typescript": "2.1.4"
  },
  "greenkeeper": {
    "branchPrefix": "refresh-dep-update-",
    "label": "refresh-dep-update"
  },
  "lint-staged": {
    "*.ts": "tslint",
    "*.css": "stylelint"
  },
  "pre-commit": "lint-staged",
  "stylelint": {
    "extends": "stylelint-config-standard"
  }
}
